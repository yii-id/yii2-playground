{
    "name": "yii-id/yii2-playground",
    "description": "Yii 2 Playground",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yii-id/yii2-playground/issues?state=open",
        "source": "https://github.com/yii-id/yii2-playground"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.4",
        "yiisoft/yii2-bootstrap": "~2.0",
        "yiisoft/yii2-swiftmailer": "~2.0",
        "yiisoft/yii2-jui": "~2.0",
        "yiisoft/yii2-twig": "~2.0",
        "deesoft/yii2-console": "~1.0",
        "deesoft/yii2-tools": "~1.0",
        "deesoft/yii2-handsontable": "~1.0",
        "mdmsoft/yii2-admin": "~2.0",
        "mdmsoft/yii2-captcha": "~1.0",
        "mdmsoft/yii2-upload-file": "~1.0",
        "mdmsoft/yii2-ar-behaviors": "~1.0",
        "mdmsoft/yii2-format-converter": "~1.0",
        "mdmsoft/yii2-autonumber": "~1.0",
        "mdmsoft/yii2-widgets": "~1.0",
        "mdmsoft/yii2-action-filter": "~1.0",
        "rmrevin/yii2-fontawesome": "~2.0",
        "almasaeed2010/adminlte": "~2.0",
        "scrivo/highlight.php": "~8.0",
        "bower-asset/angular": "1.4.x",
        "bower-asset/angular-route": "1.4.x",
        "bower-asset/angular-resource": "1.4.x"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0",
        "deesoft/yii2-gii": "~1.0"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
